{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "experimentalDecorators": true,
    "baseUrl": ".",
    "paths": {
      "*": ["./*"],
      "@assets/*": ["src/assets"],
      "@components/*": ["src/components"],
      "@hooks/*": ["src/hooks"],
      "@type/*": ["src/type"],
      "@stores/*": ["src/stores"],
      "@services/*": ["src/services"],
      "@navigation/*": ["src/navigation"],
      "@screens/*": ["src/screens"],
      "@constants/*": ["src/consts"],
    }
  }
}
