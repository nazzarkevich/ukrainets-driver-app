{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "experimentalDecorators": true,
    "baseUrl": ".",
    "paths": {
      "*": ["./*"],
      "@assets/*": ["./assets"],
      "@components/*": ["./components"],
      "@hooks/*": ["./hooks"],
      "@type/*": ["./type"],
      "@stores/*": ["./stores"],
      "@services/*": ["./services"],
      "@navigation/*": ["./navigation"],
      "@screens/*": ["./screens"],
      "@constants/*": ["./constants"],
    }
  }
}
